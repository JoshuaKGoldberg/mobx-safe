{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Jest: Unit Test File",
			"program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
			"args": [
				"--runInBand",
				"${fileBasename}"
			],
			"runtimeArgs": [
				"--nolazy"
			],
			"console": "internalConsole",
			"env": {
				"NODE_ENV": "test"
			},
			"cwd": "${workspaceRoot}",
			"sourceMaps": true,
		}
	]
}